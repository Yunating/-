#include<iostream>
#include<string>
using namespace std;
struct Student
{
string name;
int age;
int score;
};
int main(){
	struct Student stuArray[3] = 
	{
	
		{"约翰",18,100},
		{"四个",19,60},
		{"jne",20,20}
	};
	stuArray[2].name = "炸熘";
		stuArray[2].age = 80;
		stuArray[2].score = 60;

		for(int i = 0;i<3;i++){
		
		cout<<"姓名：   "<<stuArray[i].name
			<<" 年龄    "<<stuArray[i].age
			<<" 分数    "<<stuArray[i].score<<endl;
		
		}




system("pause");
return 0;
}
